		newPL = newPL - (order.getPL() - order.getPlannedCapacity());
	}	

		
	t.log("Time: " + time() + " actual start " + order.getActualStart() + " actual end " + order.getActualEnd(), Tools.LogPLSLValues);
	t.log("Order Id: "  + order.getOrderId(), Tools.LogPLSLValues);
