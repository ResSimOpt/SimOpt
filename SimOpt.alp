			<InitialSetupCode><![CDATA[
ForecastHistory.DeleteFile(ForecastHistory.FC_HISTORY_FILENAME);
ForecastHistory.DeleteFile(MRP.FILENAME_MRP_TABLE);
ForecastHistory.DeleteFile(MRP.FILENAME_MRP_TABLE_HORIZONTAL);
