                           cplex.sum(W[p][t-1], R[p][t]));   
                                                                                                                                    
            }			             											
         }
         
                  
          for (int p = 0; p < iQd.size(); p++) {
            IloLinearNumExpr num_expr_Sum_Binaries = cplex.linearNumExpr();            
