		            		if(releaseTmp[p][t] > 0){
		            			o = o + iQd.get(p).getQuantity() + "*" + RI[p][releaseTmp[p][t]-1].getName() +  "+";	            			
		            			num_expr.addTerm(iQd.get(p).getQuantity(),RI[p][releaseTmp[p][t]-1]);		            			
		            					            			
		            			sizeExpr++;		            					            					            			
		            		}			        
	            		}
					}
					
				if(o.length() > 1){
		            if(sizeExpr > 0){
		            	//Constraint Release Size 
			            cplex.addEq(R[pp][t],num_expr);    
			                  
			        }

