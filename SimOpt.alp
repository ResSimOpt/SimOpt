         for (int pp = 0; pp < itemsToPlan.size(); pp++) {
         	IloLinearNumExpr cf = cplex.linearNumExpr();				         
	       for (int t = 0; t < main.simParams.getMRPPlanningHorizon(); t++) {	            	            
			       	String o = "";
			       	int sizeExpr = 0;
			        IloLinearNumExpr num_expr = cplex.linearNumExpr();
			                    
	                for (int p = 0; p < iQd.size(); p++) {
