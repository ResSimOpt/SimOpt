         	  }//for
         	           	           	           	  
			  //PLt <= Lt + Ct (Eq14)
         	  //Constraint Pl musst be smaller as SL for all t
         	  //CF partition 1 (numerical example)        	  
         	  cplex.addLe(
         	  	cplex.sum(cf3,cplex.prod(setupTime,sumRIs)), //LHS         	  
	         	cplex.sum(cplex.sum(cf4,cplex.prod(setupTime,sumRIs)),C[t]));         	  
         	  
			  cf.clear();
			  cf1.clear();
 			  cf2.clear();
 			  cf3.clear();
 			  cf4.clear();
         }
                         
    if(time() == 0)                 
