	        for (ProductionOrder order: get_Main().productionOrders) {
	            traceln(ProductionOrder.GetCSVString(order));
	        }        
	        traceln("**************************");                							
        }
		
		//get_Main().Function_OrderRelease();
		
		cplex.deleteNames();        
